\cs_new_protected:Nn \@@_set_item_width:
  {
    \dim_set:Nn \itemwidthdim
      {
        \linewidth
      }
    \dim_compare:nT
      {
        \dim_use:c { l_@@_listoffset_ \_@@_level: _dim } != \c_zero_dim
      }
      {
        \dim_sub:Nn \itemwidthdim
          {
            \dim_use:c { l_@@_listoffset_ \_@@_level: _dim }
          }
      }
