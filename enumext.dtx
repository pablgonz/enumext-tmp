      {
        \int_mod:nn {  \g_@@_item_count_all_vii_int } { \l_@@_columns_vii_int }
      }
      =
      { 0 }
      {
        \par
        \vspace{ -\l_@@_itemsep_vii_skip }
        \int_gzero:N \g_@@_item_count_all_vii_int
      }
  }
