        \@@_mini_addvspace_viii:
        \nointerlineskip\noindent
        \begin{@@_mini_env*}{ \l_@@_minipage_left_viii_dim }
      }
   }
\cs_new_protected:Nn \@@_stop_mini_viii:
  {
    \bool_if:NT \l_@@_minipage_active_viii_bool
